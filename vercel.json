{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/portfolio",
      "destination": "/portfolio.html"
    },
    {
      "source": "/download",
      "destination": "/downloads.html"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
